{% extends 'base.html.twig' %}

{% block title %}Params{% endblock %}

{% block body %}
    <h1>Params</h1>

    <table class="table">
        <tbody>
            {# <tr>
                <th>Id</th>
                <td>{{ param.id }}</td>
            </tr> #}
            <tr>
                <th>TextParam</th>
                <td>{{ param.textParam }}</td>
            </tr>
            <tr>
                <th>LogoParam</th>
                <td>{{ param.logoParam }}</td>
            </tr>
            <tr>
